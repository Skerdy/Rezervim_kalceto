<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.skerdi.rezervim_kalceto.Kerko">

    <Spinner
        android:id="@+id/spinner_fusha"
        android:layout_width="234dp"
        android:layout_height="46dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="21dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="70dp"
        app:layout_constraintBottom_toTopOf="@+id/button6" />

    <TextView
        android:id="@+id/fushatxt"
        android:layout_width="160dp"
        android:layout_height="54dp"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:text="Fusha"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner_fusha" />

    <Spinner
        android:id="@+id/spinner_kompleks"
        android:layout_width="234dp"
        android:layout_height="46dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/fushatxt" />

    <TextView
        android:id="@+id/kompleksitxt"
        android:layout_width="160dp"
        android:layout_height="54dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:text="Kompleksi"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner_kompleks" />

    <Spinner
        android:id="@+id/spinner_qytet"
        android:layout_width="234dp"
        android:layout_height="46dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="21dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toTopOf="@+id/kompleksitxt" />

    <TextView
        android:id="@+id/qytetitxt"
        android:layout_width="117dp"
        android:layout_height="51dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="16dp"
        android:text="Qyteti"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner_qytet"
        app:layout_constraintVertical_bias="0.127" />

    <Button
        android:id="@+id/button6"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="Vazhdo"
        tools:layout_editor_absoluteX="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp" />
</android.support.constraint.ConstraintLayout>
